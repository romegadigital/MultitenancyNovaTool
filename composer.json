{
    "name": "romegadigital/multitenancy-nova-tool",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "vyuldashev/nova-permission": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "RomegaDigital\\MultitenancyNovaTool\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "RomegaDigital\\MultitenancyNovaTool\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
